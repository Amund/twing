{
  "author": "Eric MORAND <eric.morand@gmail,com>",
  "name": "twing",
  "description": "TypeScript port of the Twig templating language.",
  "version": "0.1.3",
  "homepage": "https://github.com/ericmorand/twing",
  "licenses": [
    {
      "type": "BSD-2-Clause",
      "url": "https://raw.github.com/twigjs/twig.js/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ericmorand/twing"
  },
  "main": "build/node/twing.js",
  "engines": {
    "node": ">=6.0"
  },
  "scripts": {
    "postversion": "git push origin master --tags",
    "test": "TS_NODE_CACHE=0 npm run fastest",
    "fastest": "tape -r ts-node/register/type-check 'test/tests/**/*.ts' | tap-mocha-reporter spec",
    "fastest2": "tape -r ts-node/register/type-check 'test/tests/integration-tests.ts' | tap-mocha-reporter spec",
    "build": "tsc",
    "cover": "nyc npm t",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run build && node tasks/build.js"
  },
  "dependencies": {
    "camelcase": "^4.1.0",
    "capitalize": "^1.0.0",
    "dateformat-converter": "^2.0.1",
    "fs-extra": "^5.0.0",
    "hash.js": "^1.1.3",
    "htmlspecialchars": "^1.0.5",
    "iconv-lite": "^0.4.19",
    "is-integer": "^1.0.7",
    "is-number": "^4.0.0",
    "levenshtein": "^1.0.5",
    "locutus": "^2.0.9",
    "luxon": "^0.3.1",
    "merge": "^1.2.0",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "object-hash": "^1.2.0",
    "pad": "^2.0.3",
    "parse-function": "^5.0.23",
    "regex-parser": "^2.2.8",
    "secure-filters": "^1.1.0",
    "sha.js": "^2.4.9",
    "snake-case": "^2.1.0",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "@types/luxon": "^0.2.2",
    "@types/node": "^9.3.0",
    "@types/tape": "^4.2.30",
    "coveralls": "^3.0.0",
    "fs-extra": "^5.0.0",
    "fs-finder": "^1.8.1",
    "node-hook": "^1.0.0",
    "nyc": "^11.4.1",
    "rollup": "^0.54.1",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-typescript2": "^0.10.0",
    "tap": "^11.0.1",
    "tap-diff": "^0.1.1",
    "tap-mocha-reporter": "^3.0.6",
    "tap-nyc": "^1.0.3",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tokenizer": "1.1.x",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "nyc": {
    "all": true,
    "lines": 100,
    "branches": 100,
    "functions": 100,
    "extension": [
      ".ts"
    ],
    "include": "src",
    "reporter": [
      "text-summary",
      "html"
    ],
    "require": [
      "ts-node/register/type-check"
    ]
  }
}
